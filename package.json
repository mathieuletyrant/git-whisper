{
  "name": "git-whisper",
  "version": "1.1.7",
  "description": "A CLI tool for GitHub operations",
  "keywords": [
    "github",
    "git",
    "cli",
    "ai-assistant",
    "code-review",
    "pull-request",
    "typescript",
    "openrouter",
    "claude"
  ],
  "author": "Mathieu Le Tyrant <mathieu.letyrant@gmail.com>",
  "main": "dist/index.js",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/mathieuletyrant/git-whisper",
  "bin": {
    "git-whisper": "dist/index.js",
    "gw": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "start": "node dist/index.js",
    "prepare": "npm run build",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean:dist": "rm -rf dist",
    "prepublishOnly": "npm run clean:dist && npm run build"
  },
  "dependencies": {
    "axios": "1.7.7",
    "commander": "11.0.0",
    "enquirer": "2.4.1"
  },
  "devDependencies": {
    "@types/node": "20.5.0",
    "@typescript-eslint/eslint-plugin": "8.13.0",
    "@typescript-eslint/parser": "8.13.0",
    "eslint": "9.14.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "prettier": "3.3.3",
    "typescript": "5.1.6"
  }
}
